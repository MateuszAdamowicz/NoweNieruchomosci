@model Models.ViewModels.ContactEmailViewModel

<body>
    <div class="col-md-12">
        <h2>Pytanie od użytkownika</h2>
        <hr />
        <h4>Dane użytkownika:</h4>
        <b>Imię i nazwisko:</b> @Model.FullName<br />
        <b>Telefon:</b> @Model.PhoneNumber<br />
        <b>Email:</b> @Model.Email<br />
        <b>Treść wiadomości:</b><br />
        <p>@Model.Body</p>
    </div>
</body>